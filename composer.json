{
    "name": "sandwave-io/cloud-sdk-php",
    "description": "PCextreme API Cloud SDK",
    "keywords": ["pcextreme", "servers", "sdk", "compute"],
    "license": "proprietary",
    "authors": [
        {
            "name": "Lesley de Smet",
            "email": "lesley@sandwave.io"
        },
        {
            "name": "Jesse Kramer",
            "email": "jesse@sandwave.io"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.0",
        "guzzlehttp/psr7": "^1.4.1",
        "ramsey/uuid": "^3.8",
        "symfony/yaml": "^4.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "mockery/mockery": "^1.3",
        "phpstan/phpstan": "^0.12.18",
        "phpunit/phpunit": "^7.5",
        "uptimeproject/php-cs-fixer-config": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "SandwaveIo\\CloudSdkPhp\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SandwaveIo\\CloudSdkPhp\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
