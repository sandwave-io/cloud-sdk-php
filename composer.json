{
    "name": "sandwave-io/cloud-sdk-php",
    "description": "PCextreme API Cloud SDK",
    "keywords": ["pcextreme", "servers", "sdk", "compute"],
    "license": "proprietary",
    "authors": [
        {
            "name": "Lesley de Smet",
            "email": "lesley@sandwave.io"
        },
        {
            "name": "Jesse Kramer",
            "email": "jesse@sandwave.io"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.5",
        "guzzlehttp/psr7": "^1.6.1",
        "symfony/yaml": "^4.4"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.18",
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "PCextreme\\CloudSdkPhp\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PCextreme\\CloudSdkPhp\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
